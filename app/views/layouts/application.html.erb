<!DOCTYPE html>
<html>

  <head>
    <title>Slackoverflowed</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <link href='https://fonts.googleapis.com/css?family=Work+Sans:400,800' rel='stylesheet' type='text/css'>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">

  </head>
  <!-- <div class="container page"> -->
  <body>
    <nav class="navbar navbar-default" role="navigation">
      <div class="navbar-header">
        <a class="navbar-brand brand" href="/questions">
          <%= image_tag 'SlackOverflow_LogoMark.png'%>
        </a>
        <%= yield(:question_bar) %>

        <div class="nav_log_out">
          <% if user_signed_in? %>
          Hello,
          <p><%= current_user.username %>!</p>
          <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'nav nav-pills'  %>
          <% end %>
        </div>
      </div>
    </nav>
    <div class="content">

      <!-- user logout -->
      <%= yield %>
      <script id="dsq-count-scr" src="//slackflow.disqus.com/count.js" async></script>
    </div>
  </body>
  <!-- </div> -->
</html>
